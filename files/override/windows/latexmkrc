$xelatex = 'xelatex -halt-on-error -synctex=1 -interaction=nonstopmode -file-line-error %O %S';
$xelatex_silent = 'xelatex -synctex=1 -halt-on-error -interaction=batchmode';
$bibtex = 'upbibtex %O %B';
$biber = 'biber --bblencoding=utf8 -u -U --output_safechars';
$dvipdf = 'dvipdfmx %O -o %D %S';
$makeindex = 'mendex %O -o %D %S';
$max_repeat = 10;
$pdf_mode = 3;
$out_dir = 'out';
